{{>layout/header}}

<div id="posts_list">
    <table id="table" class="table table-striped">
        <thead id="thead">
        <tr>
            <th>닉네임</th>
            <th>아이디</th>
            <th>이메일</th>
            <th>가입일</th>
            <th>수정일</th>
            <th>권한</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#user}}
            <tr>
                <td>{{nickname}}</td>
                <td>{{username}}</a></td>
                <td>{{email}}</td>
                <td>{{created_date}}</td>
                <td>{{modified_date}}</td>
                <td>{{role}}</td>
            </tr>
        {{/user}}
        </tbody>
    </table>
</div>
{{>layout/footer}}
